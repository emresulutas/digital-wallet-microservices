{
  "consumer": {
    "name": "ApiGateway"
  },
  "interactions": [
    {
      "description": "Kullanici listesi istegi",
      "providerStates": [
        {
          "name": "Veritabaninda kullanicilar var"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/users"
      },
      "response": {
        "body": [
          {
            "email": "test@test.com",
            "fullName": "Emre Can",
            "id": 1
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$[*].email": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$[*].fullName": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "16.0.2"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "CoreBankingService"
  }
}